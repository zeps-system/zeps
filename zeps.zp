(load "vendor/cli/cli")
(load "vendor/semver/sv.zp")
(import-all "semver")
(load "zeps/all")


(import-all "cli")
(import-all "zeps")

(define env
  (cli:create
    (list
      (cli:command "install" zeps:install zeps:install-info)
      (cli:command "remove" zeps:remove zeps:remove-info)
      (cli:command "register" zeps:register zeps:register-info)
      (cli:command "upgrade"  zeps:upgrade zeps:upgrade-info))))

(cli:play env)
